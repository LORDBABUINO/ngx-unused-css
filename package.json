{
  "name": "ngx-unused-css",
  "version": "3.0.4",
  "description": "Detect unused CSS in angular components",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "lint": "eslint --ext .ts src/**",
    "lint:fix": "eslint --fix --ext .ts src/**",
    "test": "jest src/** --runInBand --coverage",
    "test:watch": "jest src/** --runInBand --watch",
    "e2e": "jest e2e/test.ts",
    "e2e:manual": "cd e2e && npm test",
    "e2e:init": "node dist/index --init",
    "prepare": "husky install"
  },
  "keywords": [
    "angular",
    "unused",
    "css",
    "detect",
    "find"
  ],
  "author": "Ivan Blazevic",
  "license": "ISC",
  "dependencies": {
    "chalk": "4.0.0",
    "jsdom": "19.0.0",
    "meow": "9.0.0",
    "prompts": "2.4.2",
    "purgecss": "4.0.3",
    "sass": "^1.72.0",
    "table": "6.8.0"
  },
  "bin": {
    "ngx-unused-css": "./dist/index.js"
  },
  "bugs": "https://github.com/ivanblazevic/ngx-unused-css/issues",
  "homepage": "https://github.com/ivanblazevic/ngx-unused-css",
  "repository": {
    "type": "git",
    "url": "https://github.com/ivanblazevic/ngx-unused-css"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.6",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^20.12.2",
    "@types/prompts": "^2.0.14",
    "@types/table": "^6.3.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "mock-fs": "^5.2.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3"
  }
}
